<UserControl x:Class="CenterSwimming.Components.UserControls.TimeUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CenterSwimming.Components.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="150">

    <Button Background="White" Foreground="Black" x:Name="TimeBtn">
        <Button.Resources>
            <Style TargetType="Border">
                <Setter Property="CornerRadius" Value="7"/>
            </Style>
        </Button.Resources>
    </Button>

</UserControl>
